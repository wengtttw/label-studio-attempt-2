    "ui:serve": "nx storybook storybook",
    "ui:test:component": "nx test-component ui",
    "lint": "biome check --apply .",
    "lint-scss": "yarn stylelint '**/*.scss' --fix",
    "ls:dev": "nx run labelstudio:serve:development",
